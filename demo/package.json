{
  "name": "bulma",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build && node src/scripts/generate-sitemap.js",
    "start": "next start",
    "lint": "eslint",
    "generate-sitemap": "node src/scripts/generate-sitemap.js"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "./src/app/globals.css",
    "tailwindFunctions": [
      "clsx"
    ]
  },
  "dependencies": {
    "@tailwindplus/elements": "^1.0.21",
    "clsx": "^2.1.1",
    "fast-glob": "^3.2.12",
    "mixpanel-browser": "^2.73.0",
    "next": "^16.1.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5"
  }
